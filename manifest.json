{
  "name": "Unlock SO",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Unlocking articles",
  "homepage_url": "https://github.com/L3t1fus/chrome_so_unlocker",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "images/128.png",
    "64": "images/64.png"
  },
  "browser_action": {
    "default_title": "Unlock !"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": ["inject.js", "gsoi.js"]
}